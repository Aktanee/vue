{
  "name": "@zeit-ui/vue",
  "version": "0.5.1",
  "description": "A popular UI framework for Vue",
  "scripts": {
    "start": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name zeit-ui ./packages/index.js",
    "clear": "rm -rf dist",
    "now-build": "NODE_ENV=production yarn run build",
    "release": "yarn run clear && yarn run build:lib && yarn publish --access public"
  },
  "dependencies": {
    "@zeit-ui/style": "0.7.2",
    "prismjs": "^1.15.0",
    "vue": "^2.6.6",
    "vue-prism-component": "^1.1.1",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.0",
    "@commitlint/config-conventional": "^7.5.0",
    "@commitlint/prompt-cli": "^7.5.0",
    "@commitlint/travis-cli": "^7.5.0",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-haifengfe": "^0.1.5",
    "husky": "^1.3.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "terser": "^3.14.1",
    "vue-cli-plugin-pug": "^1.0.7",
    "vue-markdown-loader": "^2.4.1",
    "vue-template-compiler": "^2.6.6"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "not ie <= 8"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  },
  "engines": {
    "node": ">= 8.1.0",
    "npm": ">= 5.0.0"
  },
  "files": [
    "LICENSE",
    "dist/",
    "packages/"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "vue component",
    "zeit",
    "vue ui"
  ],
  "license": "MIT",
  "main": "dist/zeit-ui.common.js",
  "module": "dist/zeit-ui.common.js",
  "peerDependencies": {
    "vue": "^2.5.16"
  },
  "style": "dist/zeit-ui.css",
  "unpkg": "dist/zeit-ui.umd.js"
}
