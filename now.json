{
  "name": "zeit-vue",
  "version": 2,
  "alias": "zeit-vue",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "^/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "^/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}
